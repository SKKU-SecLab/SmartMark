pragma solidity ^0.6.8;




contract GameeVouchersRedeemer is ERC1155TokenReceiver, Ownable, Pausable {

    using SafeMath for uint256;

    event VoucherRedeemedSingle(address _from, uint256 _id, uint256 _value, uint256 _gameeAmount);
    event VoucherRedeemedBatch(address _from, uint256[] _ids, uint256[] _values, uint256 _gameeAmount);

    IERC1155InventoryBurnable private gameeVouchersContract;
    IERC20Transferrable private gameeContract;

    constructor(address gameeVouchersContract_, address gameeContract_) public {
        require(gameeVouchersContract_ != address(0) && gameeContract_ != address(0), "Redeemer: zero address");
        gameeVouchersContract = IERC1155InventoryBurnable(gameeVouchersContract_);
        gameeContract = IERC20Transferrable(gameeContract_);
    }

    function onERC1155Received(
        address, /*operator*/
        address from,
        uint256 id,
        uint256 value,
        bytes calldata /*data*/
    ) external virtual override whenNotPaused returns (bytes4) {

        require(msg.sender == address(gameeVouchersContract), "Redeemer: wrong inventory");
        require(id != 0, "Redeemer: invalid voucher id");
        
        gameeVouchersContract.burnFrom(address(this), id, value);
        uint256 gameeAmount = id.mul(value);
        gameeContract.transfer(from, gameeAmount);

        emit VoucherRedeemedSingle(from, id, value, gameeAmount);

        return _ERC1155_RECEIVED;
    }

    function onERC1155BatchReceived(
        address, /*operator*/
        address from,
        uint256[] calldata ids,
        uint256[] calldata values,
        bytes calldata /*data*/
    ) external virtual override whenNotPaused returns (bytes4) {

        require(msg.sender == address(gameeVouchersContract), "Redeemer: wrong inventory");
        
        uint256 gameeAmount = 0;
        for (uint256 i = 0; i < ids.length; i++) {
            gameeAmount = gameeAmount.add(ids[i].mul(values[i]));
        }
        gameeVouchersContract.batchBurnFrom(address(this), ids, values);
        gameeContract.transfer(from, gameeAmount);

        emit VoucherRedeemedBatch(from, ids, values, gameeAmount);

        return _ERC1155_BATCH_RECEIVED;
    }

    function withdraw(uint256 amount) external onlyOwner {

        require(amount != 0, "Redeemer: invalid amount");
        gameeContract.transfer(msg.sender, amount);
    }
}

interface IERC20Transferrable {

    function transfer(address recipient, uint256 amount) external returns (bool);


    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);

}

interface IERC1155InventoryBurnable {

    function burnFrom(
        address from,
        uint256 id,
        uint256 value
    ) external;


    function batchBurnFrom(
        address from,
        uint256[] calldata ids,
        uint256[] calldata values
    ) external;

}