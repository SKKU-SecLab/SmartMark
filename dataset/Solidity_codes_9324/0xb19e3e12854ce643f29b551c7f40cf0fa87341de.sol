        string memory _name,
        string memory _symbol,
        LibPart.Part[] memory _royalties,
        Trait[] memory _traits,
        uint256 _total,
        uint256 _maxValue
    ) external {

        bytes memory data = abi.encodeWithSelector(
            ERC721Gen(0).__ERC721Gen_init.selector,
            _name,
            _symbol,
            baseURI,
            transferProxy,
            operatorProxy,
            _royalties,
            _traits,
            _total,
            _maxValue
        );
        ProxyAdmin admin = new ProxyAdmin();
