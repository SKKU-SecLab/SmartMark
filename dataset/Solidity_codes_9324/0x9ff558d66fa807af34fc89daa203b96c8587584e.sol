library SafeCast {

    function toUint128(uint256 x) internal pure returns(uint128) {

        require(
            x <= type(uint128).max,
            "SafeCast: Cast overflow"
        );
        return uint128(x);
    }

    function toInt256(uint256 x) internal pure returns(int256) {

        require(
            x <= uint256(type(int256).max),
            "SafeCast: Cast overflow"
        );
        return int256(x);
    }
}


