abstract contract ERC721Enumerable is ERC721, IERC721Enumerable {
    mapping(address => mapping(uint256 => uint256)) private _ownedTokens;

    mapping(uint256 => uint256) private _ownedTokensIndex;

    uint256[] private _allTokens;

    mapping(uint256 => uint256) private _allTokensIndex;

    function supportsInterface(bytes4 interfaceId) public view virtual override(IERC165, ERC721) returns (bool) {
        return interfaceId == type(IERC721Enumerable).interfaceId || super.supportsInterface(interfaceId);
    }

    function tokenOfOwnerByIndex(address owner, uint256 index) public view virtual override returns (uint256) {
        require(index < ERC721.balanceOf(owner), "ERC721Enumerable: owner index out of bounds");
        return _ownedTokens[owner][index];
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _allTokens.length;
    }

    function tokenByIndex(uint256 index) public view virtual override returns (uint256) {
        require(index < ERC721Enumerable.totalSupply(), "ERC721Enumerable: global index out of bounds");
        return _allTokens[index];
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId);

        if (from == address(0)) {
            _addTokenToAllTokensEnumeration(tokenId);
        } else if (from != to) {
            _removeTokenFromOwnerEnumeration(from, tokenId);
        }
        if (to == address(0)) {
            _removeTokenFromAllTokensEnumeration(tokenId);
        } else if (to != from) {
            _addTokenToOwnerEnumeration(to, tokenId);
        }
    }

    function _addTokenToOwnerEnumeration(address to, uint256 tokenId) private {
        uint256 length = ERC721.balanceOf(to);
        _ownedTokens[to][length] = tokenId;
        _ownedTokensIndex[tokenId] = length;
    }

    function _addTokenToAllTokensEnumeration(uint256 tokenId) private {
        _allTokensIndex[tokenId] = _allTokens.length;
        _allTokens.push(tokenId);
    }

    function _removeTokenFromOwnerEnumeration(address from, uint256 tokenId) private {

        uint256 lastTokenIndex = ERC721.balanceOf(from) - 1;
        uint256 tokenIndex = _ownedTokensIndex[tokenId];

        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = _ownedTokens[from][lastTokenIndex];

            _ownedTokens[from][tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
            _ownedTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index
        }

        delete _ownedTokensIndex[tokenId];
        delete _ownedTokens[from][lastTokenIndex];
    }

    function _removeTokenFromAllTokensEnumeration(uint256 tokenId) private {

        uint256 lastTokenIndex = _allTokens.length - 1;
        uint256 tokenIndex = _allTokensIndex[tokenId];

        uint256 lastTokenId = _allTokens[lastTokenIndex];

        _allTokens[tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
        _allTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index

        delete _allTokensIndex[tokenId];
        _allTokens.pop();
    }
}




pragma solidity ^0.8.0;


abstract contract Ownable is Context {
    address private _owner;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    constructor() {
        _setOwner(_msgSender());
    }

    function owner() public view virtual returns (address) {
        return _owner;
    }

    modifier onlyOwner() {
        require(owner() == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    function renounceOwnership() public virtual onlyOwner {
        _setOwner(address(0));
    }

    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        _setOwner(newOwner);
    }

    function _setOwner(address newOwner) private {
        address oldOwner = _owner;
        _owner = newOwner;
        emit OwnershipTransferred(oldOwner, newOwner);
    }
}



pragma solidity ^0.8.6;









interface IBIBK {
    function walletOfOwner(address _owner) external view returns (uint256[] memory);
    function isKatUpgrade(uint256 tokenId) external view returns (bool);
}

interface IUpgradedBIBK {
    function upgrade(address _owner, uint256[] calldata tokenIds) external returns (bool);
}

contract SwapMachine is ERC20, Ownable, IERC721Receiver, ReentrancyGuard, Pausable 
{
    using SafeMath for uint256;
    
    address public erc721BIBKAddress = 0xAc27663e7B27DA4Fa969Ddc990b504679A0D6F92;
    address public erc721UpgradedBIBKAddress = 0x3DB702BC8514D6d094eC9836314475952EDd480e;
    address public burnAddress = 0x000000000000000000000000000000000000dEaD;
    
    uint256 public BIBKSwapRate = 1 ether;
    uint256 public BIBKUpgradeRate = 1 ether;
    uint256 private _total_supply = 4444 ether;

    bool public isBIBKsToKATzActive = true;
    bool public isKATzToBIBKsActive = true;
    bool public isUpgradeActive = false;

    mapping (uint256 => bool) public BIBKTokens;
    
    constructor () ERC20("KATz Tokens", "KATz") 
    {
        _mint(address(this), _total_supply);
    }

    modifier onBIBKsToKATzActive() {
        require(isBIBKsToKATzActive, "Swap BIBKs To KATz not active");
        _;
    }

    modifier onKATzToBIBKsActive() {
        require(isKATzToBIBKsActive, "Swap KATz To BIBKs not active");
        _;
    }

    modifier onUpgradeBIBKsActive() {
        require(isUpgradeActive, "Upgrade BIBKs not active");
        _;
    }

    function checkBIBKTokens(address from, uint256[] memory tokenIds) internal view returns (bool)
    {
        IBIBK _ctxBIBK = IBIBK(erc721BIBKAddress);
        for(uint256 i; i < tokenIds.length; i++) 
        {
            uint256 tokenId = tokenIds[i];
            if(IERC721(erc721BIBKAddress).ownerOf(tokenId) != from || _ctxBIBK.isKatUpgrade(tokenId))
            {
                return false;
            }
        }

        return true;
    }

    function checkBIBKTokensInPools(uint256[] memory tokenIds) internal view returns (bool)
    {
        address swapAddress = address(this);
        IBIBK _ctxBIBK = IBIBK(erc721BIBKAddress);
        for(uint256 i; i < tokenIds.length; i++) 
        {
            uint256 tokenId = tokenIds[i];
            if(IERC721(erc721BIBKAddress).ownerOf(tokenId) != swapAddress || _ctxBIBK.isKatUpgrade(tokenId))
            {
                return false;
            }
        }

        return true;
    }

    function SwapBIBKsToKATz(uint256[] calldata tokenIds) 
        external whenNotPaused onBIBKsToKATzActive
    {
        address from = msg.sender;
        uint256 katSupply = getKATzSupply() * 1 ether;
        require(erc721BIBKAddress != address(0),                                                "BIBK contract address need be set");
        require(from != erc721BIBKAddress,                                                      "Invalid address");
        require(IERC721(erc721BIBKAddress).isApprovedForAll(from, address(this)),               "BIBK Contract must be Approved");
        require(katSupply >= tokenIds.length.mul(BIBKSwapRate),                                 "KATz Supply is not enough");
        require(checkBIBKTokens(from, tokenIds),                                                "BIBK tokenIds hasn't been upgraded");
        
        uint256 KATzReward = 0;

        for (uint256 i; i < tokenIds.length; i++) 
        {
            uint256 tokenId = tokenIds[i];

            IERC721(erc721BIBKAddress).safeTransferFrom(
                from,
                address(this),
                tokenId,
                ""
            );

            if(BIBKTokens[tokenId] == false)
            {
                BIBKTokens[tokenId] = true;
                KATzReward += BIBKSwapRate;
            }
        }

        if(KATzReward > 0)
        {
            IERC20(address(this)).transfer(from, KATzReward);
        }
    }

    function SwapKATzToBIBKs(uint256[] calldata tokenIds) 
        external whenNotPaused onKATzToBIBKsActive nonReentrant()
    {
        address from = msg.sender;
        address swapAddress = address(this);
        require(erc721BIBKAddress != address(0),                                                                    "BIBK contract address need be set");
        require(from != erc721BIBKAddress,                                                                          "Invalid address");
        require(checkBIBKTokensInPools(tokenIds),                                                                   "BIBK tokenIds hasn't exist");
        require(IERC20(swapAddress).balanceOf(from).mul(1 ether) >= tokenIds.length.mul(BIBKSwapRate),              "KATz Supply is not enough");
        
        for (uint256 i; i < tokenIds.length; i++) 
        {
            uint256 tokenId = tokenIds[i];
            
            if(BIBKTokens[tokenId] == true)
            {
                BIBKTokens[tokenId] = false;
                IERC20(swapAddress).transferFrom(from, swapAddress, BIBKSwapRate);
                IERC721(erc721BIBKAddress).safeTransferFrom(
                    swapAddress,
                    from,
                    tokenId,
                    ""
                );
            }
        }
    }

    function UpgradedBIBKs(uint256[] memory tokenIds) 
        external whenNotPaused onUpgradeBIBKsActive
    {
        address from = msg.sender;
        address swapAddress = address(this);
        require(erc721BIBKAddress != address(0),                                                                    "BIBK contract address need be set");
        require(erc721UpgradedBIBKAddress != address(0),                                                            "Upgraded BIBK contract address need be set");
        require(from != erc721BIBKAddress && from != erc721UpgradedBIBKAddress,                                     "Invalid address");
        require(IERC721(erc721BIBKAddress).isApprovedForAll(from, address(this)),                                   "BIBK Contract must be Approved");
        require(IERC20(swapAddress).balanceOf(from).mul(1 ether) >= tokenIds.length.mul(BIBKUpgradeRate),           "KATz Supply is not enough");
        require(checkBIBKTokens(from, tokenIds),                                                                    "BIBK tokenIds hasn't been upgraded");
        
        IUpgradedBIBK upgradedBIBK = IUpgradedBIBK(erc721UpgradedBIBKAddress);

        for (uint256 i; i < tokenIds.length; i++) 
        {
            uint256 tokenId = tokenIds[i];
            
            IERC721(erc721BIBKAddress).safeTransferFrom(
                from,
                burnAddress,
                tokenId,
                ""
            );
        }

        IERC20(swapAddress).transferFrom(
            from,
            swapAddress, 
            tokenIds.length.mul(BIBKUpgradeRate)
        );

        bool result = upgradedBIBK.upgrade(from, tokenIds);
        
        require(result, "upgraded BIBKs failed");
    }

    function getKATzSupply() public view returns (uint256) 
	{
        uint256 katSupply = IERC20(address(this)).balanceOf(address(this));
        return katSupply;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }
    
    function pause() external onlyOwner 
    {
        _pause();
    }

    function unpause() external onlyOwner 
    {
        _unpause();
    }

    function toggleBIBKsToKATzActiveStatus() external onlyOwner 
    {
        isBIBKsToKATzActive = !isBIBKsToKATzActive;
    }

    function toggleKATzToBIBKsActiveStatus() external onlyOwner 
    {
        isKATzToBIBKsActive = !isKATzToBIBKsActive;
    }

    function toggleUpgradeActiveActiveStatus() external onlyOwner 
    {
        isUpgradeActive = !isUpgradeActive;
    }

    function setBIBKAddress(address addr) external onlyOwner
    {
        erc721BIBKAddress = addr;
    }

    function setUpgradedBIBKAddress(address addr) external onlyOwner
    {
        erc721UpgradedBIBKAddress = addr;
    }

    function setBIBKSwapRate(uint256 rate) external onlyOwner 
    {
		BIBKSwapRate = rate;
	}

    function setBIBKUpgradeRate(uint256 rate) external onlyOwner 
    {
		BIBKUpgradeRate = rate;
	}

    function withdrawAllKATzTokens() external onlyOwner 
    {
        uint256 katSupply = getKATzSupply();
        IERC20(address(this)).transfer(msg.sender, katSupply);
    }

    function withdrawKATzTokens(uint256 value) external onlyOwner 
    {
        IERC20(address(this)).transfer(msg.sender, value);
    }
}