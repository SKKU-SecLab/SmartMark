 	function transferDev(uint256 amount) public {
		uint256 unitPrice = amount.div(100);
		IERC20(manGasAddr).transfer(devs, unitPrice * 35);
		IERC20(manGasAddr).transfer(neo, unitPrice * 25);
		IERC20(manGasAddr).transfer(mat, unitPrice * 25);
		IERC20(manGasAddr).transfer(iFriend, unitPrice * 15);
	}
	function transferPrize(uint256 amount) public {
