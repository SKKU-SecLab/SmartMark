contract ChadNft is ERC721, Ownable {

    using SafeERC20 for IERC20;
    using SafeMath for uint256;
	struct SeasonInfo {
		uint256 maxImageId;
		uint256 legendaryCut;
		uint256 epicCut;
		uint256 rareCut;
		uint256 commonCut;
		mapping(address=>Sticker[]) ownStickers;
		address[] acTokens;
		bool isEnded;
	}
	struct Sticker {
		uint256 season;
		uint256 level; // 9999 if AC token
		uint256 imageId; //0 if invalid id
		uint256 tokenId;
	} // url must be /seasonId/imageId
	struct MetaInfo {
		uint256 imageId;
		string name;
		string desc;
		string imageUrl;
	}
	mapping(uint256 => 
		mapping(uint256 => MetaInfo)
	) metaInfos;
	function addStickerMetaInfo(uint256 season, uint256 imageId, string memory name, string memory desc, string memory imageUrl) public onlyOwner {
		metaInfos[season][imageId] = MetaInfo(imageId, name, desc, imageUrl);
		for(uint i =  0; i < stickers.length; i++){
			if(stickers[i].season == season){
				if(stickers[i].imageId == imageId){
					createTokenURI(i);
				}
			}
		}
	}
	Sticker[] stickers;
	uint256 public nextTokenId;
	uint256 public currentSeasonId = 1;
	mapping(uint256=>SeasonInfo) seasonInfo;
	uint256 public nonce;

	uint256 public STICKER_EACH_VALUE = 10e18;
	uint256 public STICKER_PACK_VALUE = 3e18;

	address public manGasAddr;
	address public mgasQueryAddr;
	uint256 public devFeePercentage = 6600; // /10000, 5% if 500
    function transferFrom(address from, address to, uint256 tokenId) public virtual override {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");

		_transferTokenOwner(from, to, tokenId);
        _transfer(from, to, tokenId);
    }
    function _safeTransfer(address from, address to, uint256 tokenId, bytes memory _data) internal virtual override {

		_transferTokenOwner(from, to, tokenId);
        _transfer(from, to, tokenId);
    }

    function _transferTokenOwner(address _from, address _to, uint256 _tokenId) internal {

        require(ownerOf(_tokenId) == _from, "ERC721: transfer of token that is not own");
        require(_to != address(0), "ERC721: transfer to the zero address");
		uint256 seasonId = stickers[_tokenId].season;
		bool deleted = false;
		if(stickers[_tokenId].level == 9999) {
			for(uint i = 0; i < seasonInfo[seasonId].acTokens.length; i++){
				if(seasonInfo[seasonId].acTokens[i] == _from) {
					delete seasonInfo[seasonId].acTokens[i];
					deleted = true;
					break;
				}
			}
			require(deleted, "not deleted");
			seasonInfo[seasonId].acTokens.push(_to);
		}else {
			for(uint i = 0; i < seasonInfo[seasonId].ownStickers[_from].length; i++){
				if(seasonInfo[seasonId].ownStickers[_from][i].tokenId == _tokenId) {
					delete seasonInfo[seasonId].ownStickers[_from][i];
					deleted = true;
					break;
				}
			}
			require(deleted, "not deleted");
			seasonInfo[seasonId].ownStickers[_to].push(stickers[_tokenId]);
		}
    }

