pragma solidity ^0.6.6;






contract MgasQuery is Ownable {

    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    address public wethAddr;// = 0x0d4a11d5eeaac28ec3f61d100daf4d40471f1852;
    address public usdtAddr;// = 0xdac17f958d2ee523a2206206994597c13d831ec7;
    address public ethUsdtPairAddr;// = 0x0d4a11d5eeaac28ec3f61d100daf4d40471f1852;
    address public manGasPairAddr;// = 0x5602B452F691143e7029a100c2889bf6d533C12A;
    address public manGasAddr;// = 0x46d5FD4A711B905ea0aaA10db31801efc75dEa56;

    function setWethAddr(address _addr) public onlyOwner{
        wethAddr = _addr;
    }
    function setUsdtAddr(address _addr) public onlyOwner{
        usdtAddr = _addr;
    }
    function setEthUsdtPairAddr(address _addr) public onlyOwner {
        ethUsdtPairAddr = _addr;
    }
    function setManGasPairAddr(address _addr) public onlyOwner {
        manGasPairAddr = _addr;
    }
    function setManGasAddr(address _addr) public onlyOwner {
        manGasAddr = _addr;
    }

    function usdtFor1Eth() public view returns(uint256) {
        uint256 usdt = IERC20(usdtAddr).balanceOf(ethUsdtPairAddr);
        uint256 eth = IERC20(wethAddr).balanceOf(ethUsdtPairAddr);
        require(eth != 0, "eth is 0");
        return usdt.div(eth); // usdt for 1eth
    }
    function usdtBalance() public view returns(uint256) {
        return IERC20(usdtAddr).balanceOf(ethUsdtPairAddr);
    }
    function mangasFor1Eth() public view returns(uint256) {

        uint256 mangas = IERC20(manGasAddr).balanceOf(manGasPairAddr);
        uint256 eth2 = IERC20(wethAddr).balanceOf(manGasPairAddr);
        require(eth2 != 0, "eth is 0");
        return mangas.div(eth2); // mangas for 1eth
    }
    function getManGasAmountWithUsdt(uint256 usdtAmount) public view returns(uint256){ 
        require(usdtAddr != address(0), "usdt is null");
        require(wethAddr != address(0), "weth is null");
        require(ethUsdtPairAddr != address(0), "eth-usdt pair is null");
        require(manGasPairAddr != address(0), "man-eth pair is null");
        require(manGasAddr != address(0), "mangas is null");
        
        return usdtAmount.mul(mangasFor1Eth().div(usdtFor1Eth()));

    }
}


