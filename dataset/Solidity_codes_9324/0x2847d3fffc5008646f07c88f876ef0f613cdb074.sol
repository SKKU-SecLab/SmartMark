	function createSticker(uint256 imageId) public returns(uint) {
		require(!seasonInfo[currentSeasonId].isEnded, "season is end");
		uint256 mgasCount = MgasQuery(mgasQueryAddr).getManGasAmountWithUsdt(STICKER_EACH_VALUE);
		uint256 amount = mgasCount;
		uint256 _prize = amount.div(10000).mul(10000 - devFeePercentage);
		uint256 devfee = amount - _prize;

		require(amount <= IERC20(manGasAddr).balanceOf(msg.sender), "low balance");
		IERC20(manGasAddr).transferFrom(msg.sender, address(this), amount);
		transferDev(devfee);
		transferPrize(amount - devfee);

		uint256 tokenId = _generateSticker(imageId);
		_mint(msg.sender, tokenId);
		createTokenURI(tokenId);

		seasonInfo[currentSeasonId].ownStickers[msg.sender].push(stickers[tokenId]);
		return tokenId;
	}
	function createPackSticker() public returns(uint[3] memory) {
		require(!seasonInfo[currentSeasonId].isEnded, "season is end");
		uint mgasCount = MgasQuery(mgasQueryAddr).getManGasAmountWithUsdt(STICKER_PACK_VALUE);
		uint256 amount = mgasCount;
		uint256 _prize = amount.div(10000).mul(10000 - devFeePercentage);
		uint256 devfee = amount - _prize;

		require(amount <= IERC20(manGasAddr).balanceOf(msg.sender), "low balance");

		IERC20(manGasAddr).transferFrom(msg.sender, address(this), amount);
		transferDev(devfee);
		transferPrize(amount - devfee);
		uint256[3] memory ret;
		uint256 tokenId;
		for(uint256 i = 0; i < 3; i++){
			tokenId = _generateRandomSticker();
			_mint(msg.sender, tokenId);
			createTokenURI(tokenId);
			seasonInfo[currentSeasonId].ownStickers[msg.sender].push(stickers[tokenId]);
			ret[i] = tokenId;
		}
		return ret;
	}
