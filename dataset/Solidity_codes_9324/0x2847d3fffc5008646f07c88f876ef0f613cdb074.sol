	function uintToString(uint v) pure internal returns (string memory str) {
        uint maxlength = 100;
        bytes memory reversed = new bytes(maxlength);
        uint i = 0;
        while (v != 0) {
            uint remainder =v % 10;
            v = v / 10;
			if(remainder == 0){
				reversed[i++] = '0';
			}
			if(remainder == 1){
				reversed[i++] = '1';
			}
			if(remainder == 2){
				reversed[i++] = '2';
			}
			if(remainder == 3){
				reversed[i++] = '3';
			}
			if(remainder == 4){
				reversed[i++] = '4';
			}
			if(remainder == 5){
				reversed[i++] = '5';
			}
			if(remainder == 6){
				reversed[i++] = '6';
			}
			if(remainder == 7){
				reversed[i++] = '7';
			}
			if(remainder == 8){
				reversed[i++] = '8';
			}
			if(remainder == 9){
				reversed[i++] = '9';
			}
        }
        bytes memory s = new bytes(i + 1);
        for (uint j = 0; j <= i; j++) {
            s[j] = reversed[i - j];
        }
        str = string(s);
    }

	function createTokenURI(uint256 _tokenId) internal {
		uint seasonId = stickers[_tokenId].season;
		uint imageId = stickers[_tokenId].imageId;
		require(seasonId != 0, "seasonId must be positive");
		string memory url = "https://chadsquad.com/metadata/season_";
		string memory seasonStr = uintToString(seasonId);
		string memory divider = "/";
		string memory imageIdStr = uintToString(imageId);
		string memory last = ".json";
		_setTokenURI(_tokenId, string(abi.encodePacked(url, seasonStr, divider, imageIdStr, last)));
	}


	function mint(address target, uint256 seasonId, uint256 imageId) public onlyOwner returns(uint256) {
		require(imageId != 0, "imageID cannot be 0");
		uint level = 1;
		if(imageId <= seasonInfo[seasonId].legendaryCut) {
			level = 4;
		}else if(imageId <= seasonInfo[seasonId].epicCut) {
			level = 3;
		}else if(imageId <= seasonInfo[seasonId].rareCut) {
			level = 2;
		}
		stickers.push(
			Sticker(
				seasonId, level, imageId, 0
			)
		);
		uint256 tokenId = stickers.length - 1;
		stickers[tokenId].tokenId = tokenId;
		_mint(target, tokenId);
		createTokenURI(stickers.length - 1);
		seasonInfo[seasonId].ownStickers[target].push(stickers[tokenId]);
		return tokenId;
	}
