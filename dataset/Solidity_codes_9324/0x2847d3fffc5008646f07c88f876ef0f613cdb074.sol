	function generateACToken(address addr) public returns(bool){
		require(seasonInfo[currentSeasonId].maxImageId < 1000, "invalid maxImageId");
		require(seasonInfo[currentSeasonId].isEnded, "season is not ended");
		for(uint i = 0; i < seasonInfo[currentSeasonId].acTokens.length; i++){
			if(seasonInfo[currentSeasonId].acTokens[i] == addr) return true;
		}

		uint8[100] memory checkList;
		Sticker[] memory userSticker = getStickerList(addr);
		for(uint i = 0; i < userSticker.length; i++){
			checkList[userSticker[i].imageId - 1] = 1;
		}
		for(uint i = 0; i < seasonInfo[currentSeasonId].maxImageId; i++){
			if(checkList[i] == 0) return false;
		}
		stickers.push( Sticker(currentSeasonId, 9999, 9999, 0));
		stickers[stickers.length - 1].tokenId = stickers.length - 1;
		seasonInfo[currentSeasonId].acTokens.push(addr);
		_mint(msg.sender, stickers.length - 1);
		createTokenURI(stickers.length - 1);
		return true;
	}
	function checkACToken(address addr) public view returns(bool){
		for(uint i = 0; i < seasonInfo[currentSeasonId].acTokens.length; i++){
			if(seasonInfo[currentSeasonId].acTokens[i] == addr) return true;
		}
		return false;
	}

    function _rand(uint max) internal returns(uint) {
        uint rand = uint(keccak256(abi.encodePacked(
            now, 
			nonce,
			block.timestamp,
            block.difficulty, 
            msg.sender)
        )) % max;
        nonce = nonce + 1;
        return rand;
    }

	function setSeasonInfo(uint seasonId, uint maxImageId, uint legendaryCut, uint epicCut, uint rareCut) onlyOwner public {
		require(legendaryCut > 0, "legendaryCut must be positive");
		require(legendaryCut < epicCut, "legendaryCut must be below epicCut");
		require(epicCut > 0, "epicCut must be positive");
		require(epicCut < rareCut, "epicCut must be below rareCut");
		require(rareCut > 0, "rareCut must be positive");
		require(rareCut < maxImageId, "rareCut must be below maxImageId");
		require(seasonId > 0, "seasonId must be positive");
		currentSeasonId = seasonId;
		
		seasonInfo[currentSeasonId].maxImageId = maxImageId;
		seasonInfo[currentSeasonId].legendaryCut = legendaryCut;
		seasonInfo[currentSeasonId].epicCut = epicCut;
		seasonInfo[currentSeasonId].rareCut = rareCut;
		seasonInfo[currentSeasonId].commonCut = maxImageId;
		seasonInfo[currentSeasonId].isEnded = false;
	}
	function setSeasonId(uint _seasonId) onlyOwner public {
		currentSeasonId = _seasonId;
	}
	function _getLevel() private returns(uint){
		uint percent = _rand(1000);
		uint threadhold = 5;
		if(percent < threadhold) { // 0.5
			return 4;
		}
		threadhold = threadhold + 20;
		if(percent < threadhold) { // 2.0
			return 3;
		} 
		threadhold = threadhold + 150;
		if(percent < threadhold) { // 15.0
			return 2;
		}
		return 1;
	}

	function _generateSticker(uint256 imageId) private returns(uint256){
		require(imageId > 0, "image id must positive");
		require(imageId <= seasonInfo[currentSeasonId].maxImageId, "invalid image id");
		
		uint level = 1;
		if(imageId <= seasonInfo[currentSeasonId].legendaryCut) {
			level = 4;
		}else if(imageId <= seasonInfo[currentSeasonId].epicCut) {
			level = 3;
		}else if(imageId <= seasonInfo[currentSeasonId].rareCut) {
			level = 2;
		}
		stickers.push(
			Sticker(
				currentSeasonId, level, imageId, 0
			)
		);
		stickers[stickers.length - 1].tokenId = stickers.length - 1;
		
		return stickers.length - 1;
	}
	function _generateRandomSticker() private returns(uint256) {
		uint level = _getLevel();
		uint min = 0;
		uint max = 0;
		if(level == 4){
			min = 1;
			max = seasonInfo[currentSeasonId].legendaryCut; // 2
		}else if(level == 3){
			min = seasonInfo[currentSeasonId].legendaryCut + 1; // 3
			max = seasonInfo[currentSeasonId].epicCut; // 6
		}else if(level == 2){
			min = seasonInfo[currentSeasonId].epicCut + 1; // 7
			max = seasonInfo[currentSeasonId].rareCut; // 12
		}else {
			min = seasonInfo[currentSeasonId].rareCut + 1; // 13
			max = seasonInfo[currentSeasonId].maxImageId; // 22
		}
		uint256 imageId = _rand(max - min + 1) + min;
		stickers.push(
			Sticker(
				currentSeasonId, level, imageId, 0
			)
		);
		stickers[stickers.length - 1].tokenId = stickers.length - 1;
		return stickers.length - 1;
	}

