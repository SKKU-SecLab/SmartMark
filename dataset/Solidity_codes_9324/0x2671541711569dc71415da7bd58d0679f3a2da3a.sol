contract Minter is Initializable {

    using SafeMath for uint256;
    address public admin;
    address public burner;
    address public mintCheckerAdmin;

    ICAP public invictusCommunityTokens;

    mapping(address => uint256) userNonce;

    event MinterIntialized(uint256 time);
    event ICTMinted(
        address indexed user,
        uint256 mintAmount,
        uint256 nonce,
        uint256 endTimestamp
    );

    event ICAPBurned(address indexed user, uint256 burnAmount);

    modifier onlyAdmin() {

        require(msg.sender == admin, "Not admin");
        _;
    }

    modifier onlyInvictusBurner() {

        require(msg.sender == burner, "Not burner");
        _;
    }

    function initialize(
        address _admin,
        address _mintCheckerAdmin,
        address _invictusCommunityTokens,
        address _burner
    ) public initializer {

        invictusCommunityTokens = ICAP(_invictusCommunityTokens);
        admin = _admin;
        burner = _burner;
        mintCheckerAdmin = _mintCheckerAdmin;
        emit MinterIntialized(now);
    }

    function changeAdmin(address _admin) external onlyAdmin {

        admin = _admin;
    }

    function changeBurner(address _burner) external onlyAdmin {

        burner = _burner;
    }

    function changeMintCheckerAdmin(address _mintCheckerAdmin)
        external
        onlyAdmin
    {

        mintCheckerAdmin = _mintCheckerAdmin;
    }

    function _hasher(
        address user,
        uint256 mintAmount,
        uint256 expiry,
        uint256 nonce,
        uint256 endTimestamp
    ) internal pure returns (bytes32) {

        return
            keccak256(
                abi.encodePacked(
                    "\x19Ethereum Signed Message:\n32",
                    keccak256(
                        abi.encodePacked(
                            user,
                            mintAmount,
                            expiry,
                            nonce,
                            endTimestamp
                        )
                    )
                )
            );
    }

    function mintTokens(
        address payable user,
        uint256 mintAmount,
        uint256 expiry,
        uint256 nonce,
        uint256 endTimestamp,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {

        require(
            ecrecover(
                _hasher(user, mintAmount, expiry, nonce, endTimestamp),
                v,
                r,
                s
            ) == mintCheckerAdmin,
            "no permission to mint"
        );
        require(now < expiry, "coupon expired");
        require(userNonce[user] == nonce, "invalid nonce");

        userNonce[user] = userNonce[user].add(1);
        invictusCommunityTokens.mint(user, mintAmount);

        emit ICTMinted(user, mintAmount, nonce, endTimestamp);
    }

    function burnTokens(uint256 burnAmount) external onlyInvictusBurner {

        invictusCommunityTokens.burnFrom(burner, burnAmount);
        emit ICAPBurned(burner, burnAmount);
    }
}